{
  "devDependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "cypress": "^12.0.2",
    "esbuild": "^0.16.4"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.0.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "json": {
      "enabled": true
    },
    "commonPath": "common",
    "stepDefinitions": [
      "[filepath]/**/*.{js,ts}",
      "[filepath].js,ts",
      "cypress/e2e/step_definitions/*.{js,ts}"
    ]
  }
}
